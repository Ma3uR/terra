{% block biloba_intl_translation_dictionary_entry_list %}
<sw-page class="biloba-intl-translation-dictionary-entry-list">
    {% block sw_manufacturer_list_search_bar %}
        <template #search-bar>
            <sw-search-bar initialSearchType="product_manufacturer"
                           :initialSearch="term"
                           @search="onSearch">
            </sw-search-bar>
        </template>
    {% endblock %}


    
    {% block biloba_intl_translation_dictionary_entry_list_language_switch %}
        <template #language-switch>
            <sw-language-switch @on-change="onChangeLanguage"></sw-language-switch>
        </template>
    {% endblock %}


    {# <template slot="smart-bar-actions">
        <sw-button :routerLink="{ name: 'biloba.intl.translation.dictionary.entry.create' }">
            {{ $t('biloba-intl-translation-dictionary.entry.list.createButtonText') }}
        </sw-button>
    </template> #}

	<template slot="content">
        {% block biloba_intl_translation_dictionary_entry_list_content %}
            <sw-entity-listing
                :isLoading="!entries"
                :columns="entryColumns"
                :repository="entryRepository"
                :showSelection="false"
                :showActions="false"
                detailRoute="biloba.intl.translation.log.detail"
                :items="entries"
                @update-records="updateTotal">
            </sw-entity-listing>
        {% endblock %}
        {% block sw_manufacturer_list_grid_loader %}
            <sw-loader v-if="isLoading"></sw-loader>
        {% endblock %}
	</template>
</sw-page>
{% endblock %}